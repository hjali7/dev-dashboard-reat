{
    "manifest_version": 3,
    "name": "LoopDashboard",
    "version": "1.2",
    "description": "یک داشبورد مدرن و خلاقانه برای توسعه‌دهندگان",
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "permissions": [
      "storage",
      "geolocation"
    ],
    "host_permissions": [
      "https://api.openweathermap.org/*"
    ]
  }